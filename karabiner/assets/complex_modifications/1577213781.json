{
    "title": "Launch apps",
    "rules": [
        {
            "description": "Launch apps by command + option + letters.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/System/Applications/Utilities/Activity Monitor.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "b",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Google Chrome.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Google Calendar.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open ~"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "g",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open 'https://github.com/nukopy'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/iTerm.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/System/Applications/Music.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Notion.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Slack.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "t",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Sourcetree.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "v",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Visual Studio Code.app'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "option"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Google Calendar.app' && open '/Applications/Notion.app'"
                        }
                    ]
                }
            ]
        }
    ]
}